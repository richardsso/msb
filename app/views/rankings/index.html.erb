<!-- Ranking per Category x User x Level -->
<div class="border">
  <p>Ranking per Category x User x Level</p>
  <% @ranking.each do |category| %>
    <p><%= category.name %></p>
    <% Category.ranking_per_category_user_level(category.name, 5).each do |player| %>
    <p><%= player.user.name %> / <%= player.level %> / <%= player.points %></p>
    <% end %>
    <p>-------------</p>
  <% end %>
</div>

