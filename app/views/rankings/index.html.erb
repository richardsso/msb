<!-- Ranking per Category x User -->
<div class="border">
  <p>Ranking per Category x User</p>
  <% @ranking.each do |category| %>
    <p><%= category.name %></p>
    <% Category.ranking_per_category_user(category.name, 5).each do |player| %>
    <p><%= player.user.name %> / <%= player.points %></p>
    <% end %>
    <p>-------------</p>
  <% end %>
</div>



<p>xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx</p>
<!-- Ranking per User -->
<div class="border">
  <p>Ranking per User</p>
