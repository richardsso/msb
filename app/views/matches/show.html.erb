<h1>Matches#show</h1>
<p>Find me in app/views/matches/show.html.erb</p>
<div class="container center">
  <div class="border center">
    <p>When: <%= @match.date %></p>
    <p>Where: <%= @match.location %></p>
    <p>Details: <%= @match.description %></p>
    <p>At: <%= @match.time %></p>
    <p>Level: <%= @match.level %></p>
    <p>Max Players: <%= @match.number_of_players %></p>
    <%= link_to "Edit", edit_match_path(@match), class: "btn btn-success" %>
    <%= link_to "Delete", match_path(@match), method: :delete, class: "btn btn-danger" %>
    <%= link_to "Matches", matches_path, class: "btn btn-success" %>
  </div>
</div>
