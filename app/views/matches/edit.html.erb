<h1>Matches#edit</h1>
<p>Find me in app/views/matches/edit.html.erb</p>
<div class="container center">
  <div class="border center">
    <%= simple_form_for [@match] do |f| %>
      <%= f.input :date %>
      <%= f.input :location %>
      <%= f.input :description %>
      <%= f.input :time %>
      <%= f.input :level %>
      <%= f.input :number_of_players %>
      <%= f.input :team_a %>
      <%= f.input :team_b %>
      <%= f.input :photo %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.submit "Update", class: "btn btn-success" %>
      <%= link_to "Matches", matches_path, class: "btn btn-success" %>
    <% end %>
  </div>
</div>
